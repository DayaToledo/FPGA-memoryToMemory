{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      },
      {
          "src": "public/**",
          "use": "@vercel/static"
      }
  ],
  "routes":[
      {
          "src": "/([a-zA-Z]*)",
          "dest": "index.js"
      },
      {
          "src": "/(.+/.+)",
          "dest": "public/$1"
      }
  ]
}